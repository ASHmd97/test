<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#60000000"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="60dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="30dp">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:padding="12dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:textStyle="bold"
                android:textColor="@color/orange"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="基本信息"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="手机型号"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/model"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="APP版本"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/appVersion"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="SDK版本"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/sdkVersion"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="播放器类型"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/playerType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="协议类型"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/streamType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="性能指标"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/cpuMem"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="videoURL"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/url"
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:maxLines="10"
                        android:singleLine="false"
                        android:inputType="textMultiLine"
                        android:textIsSelectable="true"/>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:textStyle="bold"
                android:textColor="@color/orange"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="视频信息"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="视频基本信息"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/meta"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="视频解码信息"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/vDec"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="音频解码信息"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/aDec"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textIsSelectable="true"/>
                </LinearLayout>
            </LinearLayout>
            <TextView
                android:textStyle="bold"
                android:textColor="@color/orange"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="播放状态"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="播放器标识"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/playerId"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="播放器状态"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/playerState"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="错误码"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/errorCode"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="首屏耗时"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/firstFrameCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="卡顿信息"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/frozenInfo"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="缓存信息"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/cacheInfo"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="下载速度"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/downloadSpeed"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="网络通道"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/downloadType"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <TextView
                        android:textColor="@color/white"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:text="Seek耗时"/>
                    <Space
                        android:layout_width="10dp"
                        android:layout_height="wrap_content"/>
                    <TextView
                        android:textColor="@color/white"
                        android:id="@+id/seekCost"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textIsSelectable="true"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
